{
  "name": "mafia-discord-bot",
  "version": "1.0.0",
  "description": "Notevsky Mafia Discord Bot",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "remove-commands": "cross-env NODE_ENV=production vite-node src/scripts/remove-commands.ts",
    "deploy-commands": "vite-node src/scripts/deploy-commands.ts",
    "start-bot": "vite-node src/index.ts",
    "start": "cross-env NODE_ENV=production npm run deploy-commands && npm run start-bot",
    "deploy-dev": "vite-node src/scripts/deploy-commands.ts && vite-node src/index.ts",
    "dev": "dotenv -e .env.dev -- cross-env NODE_ENV=development npm run deploy-dev"
  },
  "keywords": [],
  "author": "revom",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "i18next": "^25.2.1",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.15.29",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-node": "^3.1.4"
  }
}
